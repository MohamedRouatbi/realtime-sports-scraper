{
  "name": "realtime-sports-scraper",
  "version": "1.0.0",
  "description": "High-performance, low-latency real-time sports data pipeline with WebSocket integration and Telegram alerts",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "start:puppeteer": "node src/index-puppeteer.js",
    "dev": "node --watch src/index.js",
    "test": "node examples/test-setup.js",
    "test:setup": "node examples/test-setup.js",
    "test:puppeteer": "node examples/test-puppeteer.js",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "sports",
    "websocket",
    "telegram",
    "real-time",
    "scraper",
    "bookmaker",
    "live-data"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "node-telegram-bot-api": "^0.64.0",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
